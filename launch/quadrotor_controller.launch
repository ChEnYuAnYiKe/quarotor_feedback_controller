    <param name="position2_z" type="double" value="1.5"/>

    <param name="kp_hover_x" type="double" value="0.24"/>
    <param name="kp_hover_y" type="double" value="0.24"/>
    <param name="kp_hover_z" type="double" value="1.1"/>
    <param name="ki_hover_x" type="double" value="0.01"/>
